<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd">
  <!-- Bean declarations go here -->
  
  <bean id="senStore"
      class="edu.umich.nist.SenStore">
    <constructor-arg ref="senStoreManager" />
  </bean>
  
  <bean id="iceCommunicator"
      class="Ice.Util"
      factory-method="initialize"
      destroy-method="destroy"></bean>

  <bean id="senStoreManager"
      class="edu.umich.senstore.SenStoreMngrPrxHelper"
      factory-method="checkedCast">
    <constructor-arg ref="objectProxy" />
  </bean>

  <bean id="objectProxy"
      factory-bean="iceCommunicator"
      factory-method="stringToProxy">
    <constructor-arg value="SenStore:default -h panther.eecs.umich.edu -p 10001" />
  </bean>
</beans>