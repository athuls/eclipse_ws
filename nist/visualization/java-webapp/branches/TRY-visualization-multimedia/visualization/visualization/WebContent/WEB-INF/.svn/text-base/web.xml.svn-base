  </servlet>
  <servlet-mapping>
    <servlet-name>UnitsREST</servlet-name>
    <url-pattern>/UnitsREST</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <description></description>
    <display-name>ChannelsREST</display-name>
